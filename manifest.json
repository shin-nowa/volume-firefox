{
  "manifest_version": 2,
  "name": "Volume Control",
  "version": "1.0",
  "description": "Ajusta o volume da guia ativa.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Volume Adjustment",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/content.js"]
    }
  ],
  "icons": {
    "32": "icons/icon-32.png"
  }
}